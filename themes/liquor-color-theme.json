  "tokenColors": [
    // comment
    {
      "name": "comment",
      "scope": "comment.line.double-slash,comment.block.documentation,comment.block",
      "settings": {
        "fontStyle": "",
        "foreground": "#4f7e55c8"
      }
    },
    // JS - START
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#9f8947"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control.import", "keyword.control.from", "keyword.control.export", "support.type.object.module"],
      "settings": {
        "foreground": "#6a82a5"
      }
    },
    {
      "name": "Variable Function",
      "scope": ["variable.function"],
      "settings": {
        "foreground": "#af513a"
      }
    },
    {
      "name": "Variable Other",
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#c5a956"
      }
    },
    {
      "name": "Variable Other Object",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#c5a956"
      }
    },
    {
      "name": "Variable Other Constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#c5a956"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#9f8947"
      }
    },
    {
      "name": "Function",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#ca7b5b"
      }
    },
    {
      "name": "Function.member",
      "scope": "entity.name.function.member",
      "settings": {
        "foreground": "#aa6c42"
      }
    },
    {
      "name": "Type Name",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#6a82a5"
      }
    },
    {
      "name": "Console",
      "scope": ["support.function.console"],
      "settings": {
        "foreground": "#af513a"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#8873b3"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#f8d566"
      }
    },
    {
      "name": "js/ts ",
      "scope": "entity.other.attribute-name.js,entity.other.attribute-name.ts,entity.other.attribute-name.jsx,entity.other.attribute-name.tsx,variable.parameter,variable.language.super",
      "settings": {
        "foreground": "#80a1cb"
      }
    },
    {
      "name": "ts primitive/builtin types",
      "scope": "support.type.primitive.ts,support.type.builtin.ts,support.type.primitive.tsx,support.type.builtin.tsx",
      "settings": {
        "foreground": "#80a1cb"
      }
    },
    // JS - END

    // 标点符号 - START
    {
      "name": "punctuation definition",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#80a1cb"
      }
    },
    // 标点符号 - END

    // CSS - START
    {
      "name": "styling css pseudo-elements/classes to be able to differentiate from classes which are the same colour",
      "scope": ["entity.other.attribute-name.pseudo-element", "entity.other.attribute-name.pseudo-class", "entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#9f8947"
      }
    },
    {
      "name": "Support",
      "scope": "support.function.misc",
      "settings": {
        "foreground": "#ca7b5b"
      }
    },

    {
      "name": "Support type",
      "scope": "support.type.property-name.css,meta.property-value.css",
      "settings": {
        "foreground": "#80a1cb"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#8873b3"
      }
    },
    {
      "name": "Constants",
      "scope": "punctuation.definition.constant",
      "settings": {
        "foreground": "#8873b3"
      }
    },
    {
      "name": "Constants Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#8873b3"
      }
    },

    {
      "name": "Constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#aa6c42"
      }
    },
    // 
    {
      "name": "Variable Css",
      "scope": "variable.css,variable.scss,variable.less",
      "settings": {
        "foreground": "#c5a956"
      }
    },
    // CSS - END

    // HTML - START

    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#6A82A5"
      }
    },
    {
      "name": "Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#80a1cb"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.separator,punctuation.attribute-shorthand",
      "settings": {
        "foreground": "#6b6b6b"
      }
    },
    // HTML - END

    // VUE - START
    {
      "name": "Attribute",
      "scope": " meta.attribute.directive.vue",
      "settings": {
        "foreground": "#bababa"
      }
    },
    // VUE - END

    // JSON - START

    {
      "name": "Attribute JSON",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#6a82a5"
      }
    },
    // JSON - END
    {
      "name": "Ignore",
      "scope": " source.ignore",
      "settings": {
        "foreground": "#878686"
      }
    }
  ],
  "semanticHighlighting": true
}
